{
    "name": "kingloui/cakephp-basekit-ukusers",
    "description": "Uni Kassel users plugin for BaseKit",
    "type": "cakephp-plugin",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/KingLoui/LDAP"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0",
        "queencitycodefactory/ldap": "dev-update as dev-master",
        "kingloui/cakephp-basekit-users": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "KingLoui\\BaseKitUkUsers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KingLoui\\BaseKitUkUsers\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
